@using PSF.Dominio.Entities;
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" asp-append-version="true" />
    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Raleway:300,400,500,600,700,800,900&amp;display=swap" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <title>Listagem de Usuários</title>
</head>


@model IEnumerable<Estados>

    <table class="table table-striped table-sm" id="table-estados">
        <thead class="br-list horizontal" role="list" color="blue">
            <tr class="header">
                <br>
                <div class="title">
                    <h2>Listagem de CadastroEsf Cadastrados</h2>
                    
                </div>
                <br />
                <th class="br-item" role="listitem"><b>Nome estado</b></th>
                <span class="br-divider"></span>
            </tr>
        </thead>

        <tbody class="header">
            @foreach (var item in Model)
            {
                <tr tr class="br-list horizontal">
                <td class="br-item" role="listitem">@item.Nome</td>

                </tr>
            }
        </tbody>
    </table>


